datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

generator zod {
    provider              = "zod-prisma"
    output                = "../generated"
    relationModel         = false
    modelCase             = "camelCase"
    modelSuffix           = "Schema"
    useDecimalJs          = true
    prismaJsonNullability = false
}

generator jsonSchema {
    provider                 = "prisma-json-schema-generator"
    output                   = "../generated"
    keepRelationScalarFields = "true"
    schemaId                 = "some-schema-id"
    includeRequiredFields    = "true"
    persistOriginalType      = "true"
}

generator proto3 {
    provider = "prisma-generator-proto3"
    output   = "../generated/proto"
}

